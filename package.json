    "bootstrap": "4.5.3",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "eva-icons": "^1.1.3",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "jasmine-reporters": "^2.3.2",
    "karma-junit-reporter": "^2.0.1",
    "nebular-icons": "1.1.0",
